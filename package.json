{
  "name": "yoga",
  "version": "1.0.0",
  "private": true,
  "description": "Тренировочный макет Yoga",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint && eslint source/js/",
    "build": "gulp build",
    "start": "gulp start"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "browser-sync": "^2.26.7",
    "del": "^5.1.0",
    "eslint": "^6.6.0",
    "eslint-config-htmlacademy": "^0.5.2",
    "gulp": "^4.0.2",
    "gulp-csso": "^3.0.1",
    "gulp-flatten": "^0.4.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^6.1.1",
    "gulp-minify": "^3.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svgstore": "^7.0.1",
    "gulp-webp": "^4.0.1",
    "stylelint": "^11.1.1",
    "stylelint-config-htmlacademy": "^0.1.4"
  },
  "engines": {
    "node": "12.13"
  },
  "dependencies": {}
}
